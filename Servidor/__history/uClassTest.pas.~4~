unit uClassTest;

interface

uses uBasicClass, System.SysUtils, System.Classes, Json;

type
  tTestClass = class(TBasicClass)
  private
  //private methods
  protected
  //protected methods
  public
  //public methods
    function teste(Value: String): TJSONArray;

end;

implementation


{ tTestClass }

function tTestClass.teste(Value: String): TJSONArray;
begin
  Result := StrToJson(Value);
end;

end.
